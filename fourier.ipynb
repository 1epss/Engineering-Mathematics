   "source": [
    "### 다음 함수의 푸리에 급수를 구하라\n",
    "\n",
    "$f(x) = \\begin{cases}\n",
    "    -1\\,(-\\pi<=x<0)\\\\\n",
    "    1\\,(0<x<=\\pi)\\\\\n",
    "  \\end{cases}$,\n",
    "  \n",
    "$f(x+2\\pi) = f(x), (k > 0)$"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/latex": [
       "$\\displaystyle \\frac{4 \\sin{\\left(x \\right)}}{\\pi} + \\frac{4 \\sin{\\left(3 x \\right)}}{3 \\pi} + \\frac{4 \\sin{\\left(5 x \\right)}}{5 \\pi} + \\ldots$"
      ],
      "text/plain": [
       "FourierSeries(2*Heaviside(x) - 1, (x, -pi, pi), (0, SeqFormula(0, (_n, 1, oo)), SeqFormula((-2*Piecewise((0, Eq(_n, 0)), (-1/_n, True)) + 2*Piecewise((0, Eq(_n, 0)), (-cos(_n*pi)/_n, True)))*sin(_n*x)/pi, (_n, 1, oo))))"
      ]
     },
     "execution_count": 4,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "import sympy\n",
    "\n",
    "x = sympy.symbols('x')\n",
    "sympy.fourier_series(2*sympy.Heaviside(x)-1, (x,-sympy.pi,sympy.pi))\n"
   ]
  },
